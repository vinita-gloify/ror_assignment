<%= form_for @library do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
  
    </p>
  
    <p>
        <%= f.label :opening_time %>
        <%= f.text_area :opening_time %>

    </p>  

    <p>
        <%= f.label :closing_time %>
        <%= f.text_field :closing_time %>
  
    </p>

     
    <p>
        <%= f.submit %>
    </p>
  
<% end %>  